<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app 
   	xmlns="http://java.sun.com/xml/ns/javaee" 
   	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" 
   	version="3.0"> 

  <servlet>
    <servlet-name>location</servlet-name>
    <servlet-class>LocationJetty</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>location</servlet-name>
    <url-pattern>/gps/*</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>json</servlet-name>
    <servlet-class>LocationJson</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>json</servlet-name>
    <url-pattern>/json/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>barcode</servlet-name>
    <servlet-class>BarcodeJetty</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>barcode</servlet-name>
    <url-pattern>/barcode/*</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>wikinearjson</servlet-name>
    <servlet-class>WikinearJson</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>wikinearjson</servlet-name>
    <url-pattern>/wikinearjson/*</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>localweatherjson</servlet-name>
    <servlet-class>LocalWeatherJson</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>localweatherjson</servlet-name>
    <url-pattern>/localweatherjson/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>camera</servlet-name>
    <servlet-class>CameraJetty</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>camera</servlet-name>
    <url-pattern>/camera/*</url-pattern>
  </servlet-mapping>

</web-app>
